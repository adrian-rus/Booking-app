<h3> Bookings for <%= @zone.name %></h3>
<table class="bookings_table">
  <tr>
    <th>Start</th>
    <th>End</th>
    <th>Duration</th>
    <th>Cost</th>
    <th>Actions</th>
  </tr>
  
  <% @bookings.each do |booking| %>
  <tr>
    <td>
      <%= booking.start_time.strftime('%e %b %Y %H:%M%p') %>
    </td>
    <td>
      <%= (booking.calculate_end_time + 60).strftime('%e %b %Y %H:%M%p') %>
    </td>
    <td>
      <%= booking.duration %> hours
    </td>
     <td>
     euro <%= (booking.calculate_cost) %>
    </td>
    
    <td><%= link_to "Show Details", zone_booking_path(@zone, booking) %>
    </td>
    <td><%= link_to "Delete", zone_booking_path(@zone, booking), data: { confirm: 'Are you sure?' }, method: :delete %>
    </td>
    <td><%= link_to "Edit", edit_zone_booking_path(@zone, booking) %>
    </td>
  </tr>
  <% end %>
</table>